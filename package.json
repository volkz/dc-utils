{
  "name": "dc-utils",
  "version": "1.0.0",
  "description": "General utils for projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkz/dc-utils.git"
  },
  "author": "David de la Cruz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkz/dc-utils/issues"
  },
  "homepage": "https://github.com/volkz/dc-utils#readme",
  "devDependencies": {
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^13.1.8",
    "jsonwebtoken": "^8.5.1"
  }
}
